# API Keys for AI models
# Copy this file to .env and fill in your API keys

# OpenRouter API key for Grok and other models
OPENROUTER_API_KEY=

# Anthropic API key for Claude models (used by Aider)
ANTHROPIC_API_KEY=

# OpenAI API key for GPT models (used by Aider)
OPENAI_API_KEY=

# Project directory to mount (defaults to current directory)
PROJECT_DIR=.

# Linear App Configuration
# Required for Linear agent integration
LINEAR_API_KEY=
LINEAR_CLIENT_ID=
LINEAR_CLIENT_SECRET=
LINEAR_WEBHOOK_SECRET=
